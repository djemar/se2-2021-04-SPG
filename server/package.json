            "!**/vendor/**",
            "!**/reportWebVitals.js"
        ]
    },
    "author": "P04 Group",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cookie-parser": "^1.4.5",
        "express": "^4.17.1",
        "express-jwt": "^6.1.0",
        "express-session": "^1.17.2",
        "express-validator": "^6.5.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "morgan": "^1.10.0",
        "passport": "^0.5.0",
        "passport-local": "^1.0.0",
        "sqlite3": "^4.2.0"
    },
    "devDependencies": {
        "jest": "^27.3.1",
        "jest-sonar-reporter": "^2.0.0"
    }
