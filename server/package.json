{
    "name": "server",
    "version": "1.0.0",
    "description": "SPG REST API server",
    "main": "server.js",
    "scripts": {
        "test": "jest --coverage --watchAll"
    },
    "jest": {
        "verbose": true,
        "collectCoverage": true,
        "coverageReporters": [
            "lcov",
            "text"
        ],
        "collectiveCoverageFrom": [
